<expres><![CDATA[#$pyFunction
def GetLSProData(page_data,Cookie_Jar,m):
	import requests
	req = requests.get("https://www2.vavoo.to/live2/index").json()
	countries = []
	for group in req:
		country = group["group"]
		if country not in countries:
			countries.append(country)
	return sorted(countries) 
]]></expres>
